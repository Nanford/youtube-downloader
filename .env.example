# YouTube 下载器环境配置
# 复制此文件为 .env 并填入实际值

# 基础配置
SECRET_KEY=your_secret_key_here_replace_with_random_string
FLASK_DEBUG=False
DOMAIN=yt.leenf.online
HOST=0.0.0.0
PORT=8000

# SSL 配置
CERTBOT_EMAIL=admin@leenf.online

# 下载配置
MAX_CONCURRENT_DOWNLOADS=3
DOWNLOAD_TIMEOUT=1800
MAX_CONTENT_LENGTH=20971520

# 目录配置 (Docker 内路径，通常不需要修改)
DOWNLOAD_DIR=/app/downloads
UPLOAD_DIR=/app/uploads
LOG_DIR=/app/logs

# 安全配置
ENABLE_USER_SESSIONS=True